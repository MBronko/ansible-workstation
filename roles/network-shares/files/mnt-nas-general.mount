[Unit]
Description=Mount General Data SMB Share
After=network.target

[Mount]
What=//dev.truenas.home/general-data
Where=/mnt/nas/general
Type=cifs
Options=credentials=/etc/samba/credentials,uid=1000,gid=1000,iocharset=utf8,file_mode=0775,dir_mode=0775,vers=3.0,_netdev,noauto,x-systemd.automount
TimeoutSec=30

[Install]
WantedBy=multi-user.target
